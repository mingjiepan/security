<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mjie.security.web.common.mapper.UserMapper">
    <select id="getUser"  resultType="com.mjie.security.web.common.entities.MyUser">
        select * from user where id = #{id}
    </select>


    <select id="getUserByName"  resultType="com.mjie.security.web.common.entities.MyUser">
        select * from user where username = #{name}
    </select>
</mapper>